{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "@timelet/content",
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": [
      "ES2015",
      "DOM"
    ],
    "module": "ESNext",
    "target": "ES6",
    "resolveJsonModule": true,
    "noEmit": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "dist"
  ]
}
