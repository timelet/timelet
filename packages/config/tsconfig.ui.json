{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "UI",
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["ES2015"],
    "module": "ESNext",
    "target": "es6",
    "resolveJsonModule": true,
    "noEmit": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true
  }
}
