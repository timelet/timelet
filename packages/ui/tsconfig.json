{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "UI",
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "lib": [
      "ES2015",
      "DOM"
    ],
    "module": "ESNext",
    "target": "ES6",
    "resolveJsonModule": true,
    "noEmit": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true
  }
}
